t/css" href="../pygments/trac.css" title="Trac" /><link rel="stylesheet" type="text/css" href="../pygments/vim.css" title="Vim" /><link rel="stylesheet" type="text/css" href="../pygments/vs.css" title="Vs" /><link rel="stylesheet" type="text/css" href="../pygments/xcode.css" title="Xcode" /><script type="text/javascript">
      (function($){
        window.switchStyleSheet = function(title) {
          $('link[rel="stylesheet"][title]').each(function() {
            this.disabled = true; // needed to force correct behavior for WebKit (see #10923)
            this.disabled = this.getAttribute('title') != title;
          });
        }
      })(jQuery);
      jQuery(document).ready(function($) {
        switchStyleSheet("Trac");
        $("#pygment_theme").attr("autocomplete", "off").bind('change keypress', function() {
          switchStyleSheet(this.options[this.selectedIndex].text);
        });
      });
    </script>
